// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  CPF             String @id
  nome            String
  RG              String
  data_nascimento DateTime
  nome_mae        String
  data_cadastro   DateTime @default(now())
}